---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
  <title type="text">Herr-Norbert.de</title>
  <link type="application/atom+xml" href="http://herr-norbert.de/atom.xml" rel="self" />
  <link type="text" href="http://herr-norbert.de/" rel="alternate" />
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://herr-norbert.de/</id>
  <author>
    <name>Norbert Schneider</name>
    <email>Norbert-Schneider@gmx.de</email>
  </author>
  
  {% for post in site.posts %}
  <entry>
    <title>{{ post.title }}</title>
    <link href="http://herr-norbert.de{{ post.url }}"/>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <id>http://herr-norbert.de/{{ post.id }}</id>
    <author>
      <name>Norbert Schneider</name>
      <email>Norbert-Schneider@gmx.de</email>
    </author>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}	
</feed>
